                                                <tbody>
                                                    <tr>
                                                        <td>
                                                            ID: {{card.CARD_ID}}

                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            NAME: <a href="#" editable-text="card.CARD_NAME" onbeforesave="updateField('cards','CARD_ID',card.CARD_ID,null,null,'CARD_NAME',$data)">{{ card.CARD_NAME || "empty" }}</a>

                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            DESCRIPTION: <a href="#" editable-text="card.CARD_DESCRIPTION" onbeforesave="updateField('cards','CARD_ID',card.CARD_ID,null,null,'CARD_DESCRIPTION',$data)">{{ card.CARD_DESCRIPTION || "empty" }}</a>

                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            PRIORITY: <a href="#" editable-number="card.PRIORITY" onbeforesave="updateField('cards','CARD_ID',card.CARD_ID,null,null,'PRIORITY',$data)">{{ card.PRIORITY || "empty" }}</a>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            ESTIMATE: <a href="#" editable-number="card.ESTIMATE" onbeforesave="updateField('cards','CARD_ID',card.CARD_ID,null,null,'ESTIMATE',$data)">{{ card.ESTIMATE || "empty" }}</a>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            IsBlocker?: <a href="#" editable-text="card.IS_BLOCKER" onbeforesave="updateField('cards','CARD_ID',card.CARD_ID,null,null,'IS_BLOCKER',$data)">{{ card.IS_BLOCKER || "empty" }}</a>
                                                        </td>
                                                    </tr>

                                                    <tr>
                                                        <td>
                                                            LaneId: <a href="#" editable-text="card.LANE_ID" onbeforesave="updateField('cards','CARD_ID',card.CARD_ID,null,null,'LANE_ID',$data)">{{ card.LANE_ID || "empty" }}</a>
                                                        </td>
                                                    </tr>




                                                </tbody>
                                            </table>
                                        </div>

                                    </div>

                                </div>



                            </div>


                        </tab>
                        <tab heading="RESOURCES">

                            <div align="right">
                                <input type="image" src="../images/add-resource.png" height="40" width="40" ng-click="addResource()" />
                             
                            </div>
                            <div id="wrapper" ng-show="resources.length > 0">
                                <table id="fluid">
                                    <thead>
                                        <th>ID</th>
                                        <th>NAME</th>
                                        <th>EMAIL</th>
                                        <th>TYPE</th>
                                        <th>PHONE</th>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="resource in resources">
                                            <td><a>{{resource.RESOURCE_ID}}</a>
                                            </td>
                                            <td><a href="#" editable-text="resource.RESOURCE_NAME" onbeforesave="updateField('resources','PROJECT_ID',projectID,'RESOURCE_ID',resource.RESOURCE_ID,'RESOURCE_NAME',$data)">{{ resource.RESOURCE_NAME || "empty" }}</a>
                                            </td>
                                            <td><a href="#" editable-email="resource.EMAIL" onbeforesave="updateField('resources','PROJECT_ID',projectID,'RESOURCE_ID',resource.RESOURCE_ID,'EMAIL',$data)">{{ resource.EMAIL || "empty" }}</a>
                                            </td>
                                            <td><a href="#" editable-text="resource.TYPE" onbeforesave="updateField('resources','PROJECT_ID',projectID,'RESOURCE_ID',resource.RESOURCE_ID,'TYPE',$data)">{{ resource.TYPE || "empty" }}</a>
                                            </td>
                                            <td><a href="#" editable-tel="resource.PHONE" onbeforesave="updateField('resources','PROJECT_ID',projectID,'RESOURCE_ID',resource.RESOURCE_ID,'PHONE',$data)" e-pattern="\d{3}\-\d{3}\-\d{4}" e-title="xxx-xxx-xxxx">{{ resource.PHONE || "empty" }}</a>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </tab>

                        <tab heading="STATUS">
                            <div id="kanbangraph" style="min-width: 300px; width:400px; height: 400px; margin: 0 auto">
                              
                              
                               
                                 <div class="row" style="display: table;">
                                <div class="col-sm-8" >
