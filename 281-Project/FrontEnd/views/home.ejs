                               <th> <h5 style="text-align:center;">Lower Limit</h5></th>
                               <th> <h5 style="text-align:center;">Current Count</h5></th>
                                
                                </thead>
                                 <tbody>
                      
                               
                               
                         
                               <tr style="text-align:center;" ng-repeat="lane in LaneDetails.data">
                               <td>{{lane.LANE_NAME}}</td> <td>{{lane.UPPER_LIMIT}}</td> <td>{{lane.LOWER_LIMIT}}</td>
                               
                               
                               <td> {{lane.CURRENT_COUNT}} </td></tr>
                               
                               
                        
                           
                               
                                   </tbody>
                               </table>
                               </div>
                               </div>
                              
                               
                               
                            </div>
                        </tab>
                        <tab>
                          <tab-heading style="margin:0;padding:0;">
                            <span id="backbutton" style="margin-left:50px;"><input width="45" type="image" height="40" src="../images/back.png" ng-click="viewWorkspace()"></span>
                          </tab-heading>
                        </tab>
                    </tabset>
                </div>

            </div>
        </div>

