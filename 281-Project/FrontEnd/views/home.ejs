                                    <h5 style="text-align:center;">{{lane.LANE_NAME}}</h5>

                                    <div ng-repeat="card in CardDetails.data">


                                        <div ng-show="lane.LANE_ID == card.LANE_ID && lane.PROJECT_ID == card.PROJECT_ID" style="border:1px solid black; width: 310px; margin: 10px; padding:5px;">
